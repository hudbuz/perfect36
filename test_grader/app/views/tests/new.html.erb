
<div id="test_frame" class="new_tests col-lg-8"><iframe id="newTestFrame" src="https://docs.google.com/viewer?srcid=0B2G5qRkXjY7rUEdJM0Nmbm5rYXM&pid=explorer&efh=false&a=v&chrome=false&embedded=true" width="600px" height="600px"></iframe></div>

<div id="newTests" class='test-container new_tests col-lg-4 well well-sm'>
<%= form_for @test do |f| %>
<h2>Take a New Test</h2>
<div id="answerKey">Test Code: <%= collection_select(:test, :answer_key_id, @available, :id, :code, {prompt: 'Select Test'}, {:onchange => 'changeIframe()', class: "form-control"}) %><br><br></div>
<a id="leftSection" onclick="sectionShift()" href="#"><span class="glyphicon glyphicon-arrow-left"></span></a>
 <a id="rightSection" onclick="sectionShift()" href="#"><span class="glyphicon glyphicon-arrow-right"></span></a>
 <div id="activeSection" type="hidden" value=0></div>

  <div id="takenAt"></div>
  <div id="totalScore"></div>







<div id="hiddenUserId" type="hidden" value="<%=current_user.id%>"></div>







<%= render partial:'/tests/test', locals:{format: @format, f: f} %>



<% f.submit %>
<% end %>

<button id="create">Submit</button>
</div>

<script>
$(function(){

  $('#math').hide()
  $('#reading').hide()
  $('#science').hide()
})

</script>
